    "start": "node dist/index.js",
    "consumer": "ts-node src/consumer.ts",
    "repair": "ts-node src/repair-consumer.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx src/db/seed.ts",
    "order:test": "ts-node --transpile-only src/script/order.ts",
    "load:test": "npx tsx src/script/load-test.ts",
    "test:db": "ts-node --transpile-only src/script/test-db.ts",
    "trade:test": "npx tsx script/stockTrade.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/joi": "^17.2.2",
    "amqplib": "^0.10.9",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "lodash": "^4.17.21",
    "postgres": "^3.4.8",
    "reflect-metadata": "^0.2.2",
    "uuidv7": "^1.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
